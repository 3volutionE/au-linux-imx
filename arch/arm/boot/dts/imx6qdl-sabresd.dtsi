/ {
	aliases {
		mxcfb0 = &mxcfb1;
		mxcfb1 = &mxcfb2;
		mxcfb2 = &mxcfb3;
		mxcfb3 = &mxcfb4;
	};

	memory {
		reg = <0x10000000 0x40000000>;
	};

	soc {
		pcie@01000000 {
			pwr-gpios = <&gpio3 19 0>;
			rst-gpios = <&gpio7 12 0>;
			status = "okay";
		};

		hdmi_video@00120000 {
			compatible = "fsl,imx6-hdmi-video";
			pinctrl-names = "default";
			pinctrl-0 = <&pinctrl_hdmi_hdcp_1>;
			phy_reg_vlev = <0x0294>;
			phy_reg_cksymtx = <0x800d>;
			status = "okay";
		};

		hdmi_cec@00120000 {
			compatible = "fsl,imx6-hdmi-cec";
			pinctrl-names = "default";
			pinctrl-0 = <&pinctrl_hdmi_cec_2>;
			status = "okay";
		};

		aips-bus@02000000 { /* AIPS1 */
			spba-bus@02000000 {
				uart1: serial@02020000 {
					pinctrl-names = "default";
					pinctrl-0 = <&pinctrl_uart1_1>;
					status = "okay";
				};

				ssi2: ssi@0202c000 {
					fsl,mode = "i2s-slave";
					status = "okay";
				};

				ecspi@02008000 { /* eCSPI1 */
					fsl,spi-num-chipselects = <1>;
					cs-gpios = <&gpio4 9 0>;
					pinctrl-names = "default";
					pinctrl-0 = <&pinctrl_ecspi1_2>;
					status = "okay";

					flash: m25p32@0 {
						#address-cells = <1>;
						#size-cells = <1>;
						compatible = "st,m25p32", "st,m25p";
						spi-max-frequency = <20000000>;
						reg = <0>;

						partition@0 {
							label = "U-Boot";
							reg = <0x0 0x100000>;
							read-only;
						};

						partition@40000 {
							label = "Kernel";
							reg = <0x40000 0x300000>;
						};
					};
				};
			};

			pwm1: pwm@02080000 {
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_pwm0_0>;
				status = "okay";
			};

			ldb@020e0000 {
				ipu_id = <1>;
				disp_id = <1>;
				ext_ref = <1>;
				mode = "sep1";
				sec_ipu_id = <1>;
				sec_disp_id = <0>;
				status = "okay";
			};
		};

		aips-bus@02100000 { /* AIPS2 */
			usb@02184000 { /* USB OTG */
				vbus-supply = <&reg_usb_otg_vbus>;
				pinctrl-names = "pinctrl_id";
				pinctrl-0 = <&pinctrl_usbotg_2>;
				disable-over-current;
				status = "okay";
			};

			usb@02184200 { /* USB1 */
				vbus-supply = <&reg_usb_h1_vbus>;
				status = "okay";
			};

			ethernet@02188000 {
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_enet_1>;
				phy-mode = "rgmii";
				status = "okay";
			};

			usdhc@02194000 { /* uSDHC2 */
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_usdhc2_1>;
				cd-gpios = <&gpio2 2 0>;
				wp-gpios = <&gpio2 3 0>;
				bus-width = <8>;
				no-1-8-v;
				keep-power-in-suspend;
				enable-sdio-wakeup;
				status = "okay";
			};

			usdhc@02198000 { /* uSDHC3 */
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_usdhc3_1>;
				cd-gpios = <&gpio2 0 0>;
				wp-gpios = <&gpio2 1 0>;
				bus-width = <8>;
				no-1-8-v;
				keep-power-in-suspend;
				enable-sdio-wakeup;
				status = "okay";
			};

			usdhc@0219c000 { /* uSDHC4 */
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_usdhc4_1>;
				non-removable;
				bus-width = <8>;
				no-1-8-v;
				keep-power-in-suspend;
				enable-sdio-wakeup;
				status = "okay";
			};

			audmux@021d8000 {
				status = "okay";
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_audmux_2>;
			};

			i2c@021a0000 { /* I2C1 */
				status = "okay";
				clock-frequency = <100000>;
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_i2c1_2>;

				codec: wm8962@1a {
					compatible = "wlf,wm8962";
					reg = <0x1a>;
					clocks = <&clks 169>;
					DCVDD-supply = <&reg_audio>;	/* 1.8v */
					DBVDD-supply = <&reg_audio>;	/* 1.8v */
					AVDD-supply = <&reg_audio>;	/* 1.8v */
					CPVDD-supply = <&reg_audio>;	/* 1.8v */
					MICVDD-supply = <&reg_audio>;	/* 3.3v */
					PLLVDD-supply = <&reg_audio>;	/* 1.8v */
					SPKVDD1-supply = <&reg_audio>;	/* 4.2v */
					SPKVDD2-supply = <&reg_audio>;	/* 4.2v */
					gpio-cfg = <
						0x0000 /* 0:Default */
						0x0000 /* 1:Default */
						0x0013 /* 2:FN_DMICCLK */
						0x0000 /* 3:Default */
						0x8014 /* 4:FN_DMICCDAT */
						0x0000 /* 5:Default */
					>;
				};

				ov564x: ov564x@3c {
					compatible = "fsl,ov564x";
					reg = <0x3c>;
					pinctrl-names = "default";
					pinctrl-0 = <&pinctrl_ipu1_csi0_1>;
					clocks = <&clks 169>;
					clock-names = "csi_mclk";
					DOVDD-supply = <&vgen4_reg>; /* 1.8v */
					AVDD-supply = <&vgen5_reg>;  /* 2.8v, on rev C board is VGEN3 */
					DVDD-supply = <&vgen2_reg>;  /* 1.5v*/
					pwn-gpios = <&gpio1 16 1>;   /* active low: SD1_DAT0 */
					rst-gpios = <&gpio1 17 0>;   /* active high: SD1_DAT1 */
					csi_id = <0>;
					mclk = <24000000>;
					mclk_source = <0>;
				};
			};

			i2c@021a4000 { /* I2C2 */
                                status = "okay";
                                clock-frequency = <100000>;
                                pinctrl-names = "default";
                                pinctrl-0 = <&pinctrl_i2c2_2>;

				max11801@48 {
					compatible = "fsl,max11801";
					reg = <0x48>;
					int_input = <&gpio3 26 1>;
                };

				hdmi_edid@50 {
					compatible = "fsl,imx6-hdmi-i2c";
					reg = <0x50>;
				};

				ov5640_mipi: ov5640_mipi@3c { /* i2c2 driver */
					compatible = "fsl,ov5640_mipi";
					reg = <0x3c>;
					clocks = <&clks 169>;
					clock-names = "csi_mclk";
					DOVDD-supply = <&vgen4_reg>; /* 1.8v */
					AVDD-supply = <&vgen5_reg>;  /* 2.8v, on rev C board is VGEN3 */
					DVDD-supply = <&vgen2_reg>;  /* 1.5v*/
					pwn-gpios = <&gpio1 19 1>;   /* active low: SD1_CLK */
					rst-gpios = <&gpio1 20 0>;   /* active high: SD1_DAT2 */
					csi_id = <1>;
					mclk = <24000000>;
					mclk_source = <0>;
				};

				egalax_ts@04 {
					compatible = "eeti,egalax_ts";
					reg = <0x04>;
					interrupt-parent = <&gpio6>;
					interrupts = <8 2>;
					wakeup-gpios = <&gpio6 8 0>;
				};

				pmic: pfuze100@08 {
					compatible = "fsl,pfuze100";
					reg = <0x08>;

					regulators {
						sw1a_reg: pfuze100__sw1a {
							regulator-min-microvolt = <300000>;
							regulator-max-microvolt = <1875000>;
							regulator-standby-microvolt = <18000>;
							regulator-boot-on;
							regulator-always-on;
						};

						sw1c_reg: pfuze100__sw1c {
							regulator-min-microvolt = <300000>;
							regulator-max-microvolt = <1875000>;
							regulator-boot-on;
							regulator-always-on;
						};

						sw2_reg: pfuze100__sw2 {
							regulator-min-microvolt = <800000>;
							regulator-max-microvolt = <3950000>;
							regulator-boot-on;
							regulator-always-on;
						};

						sw3a_reg: pfuze100__sw3a {
							regulator-min-microvolt = <400000>;
							regulator-max-microvolt = <1975000>;
							regulator-boot-on;
							regulator-always-on;
						};

						sw3b_reg: pfuze100__sw3b {
							regulator-min-microvolt = <400000>;
							regulator-max-microvolt = <1975000>;
							regulator-boot-on;
							regulator-always-on;
						};

						sw4_reg: pfuze100__sw4 {
							regulator-min-microvolt = <800000>;
							regulator-max-microvolt = <3950000>;
						};

						swbst_reg: pfuze100__swbst {
							regulator-min-microvolt = <5000000>;
							regulator-max-microvolt = <5150000>;
						};

						snvs_reg: pfuze100__vsnvs {
							regulator-min-microvolt = <1200000>;
							regulator-max-microvolt = <3000000>;
							regulator-boot-on;
							regulator-always-on;
						};

						vref_reg: pfuze100__vrefddr {
							regulator-boot-on;
							regulator-always-on;
						};

						vgen1_reg: pfuze100__vgen1 {
							regulator-min-microvolt = <800000>;
							regulator-max-microvolt = <1550000>;
						};

						vgen2_reg: pfuze100__vgen2 {
							regulator-min-microvolt = <800000>;
							regulator-max-microvolt = <1550000>;
						};

						vgen3_reg: pfuze100__vgen3 {
							regulator-min-microvolt = <1800000>;
							regulator-max-microvolt = <3300000>;
						};

						vgen4_reg: pfuze100__vgen4 {
							regulator-min-microvolt = <1800000>;
							regulator-max-microvolt = <3300000>;
							regulator-always-on;
						};

						vgen5_reg: pfuze100__vgen5 {
							regulator-min-microvolt = <1800000>;
							regulator-max-microvolt = <3300000>;
							regulator-always-on;
						};

						vgen6_reg: pfuze100__vgen6 {
							regulator-min-microvolt = <1800000>;
							regulator-max-microvolt = <3300000>;
						};
					};
				};
			};

			i2c@021a8000 { /* I2C3 */
				status = "okay";
				clock-frequency = <100000>;
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_i2c3_2>;

				egalax_ts@04 {
					compatible = "eeti,egalax_ts";
					reg = <0x04>;
					interrupt-parent = <&gpio6>;
					interrupts = <7 2>;
					wakeup-gpios = <&gpio6 7 0>;
				};
				max17135@48 {
					compatible = "maxim,max17135";
					reg = <0x48>;
					vneg_pwrup = <1>;
					gvee_pwrup = <1>;
					vpos_pwrup = <2>;
					gvdd_pwrup = <1>;
					gvdd_pwrdn = <1>;
					vpos_pwrdn = <2>;
					gvee_pwrdn = <1>;
					vneg_pwrdn = <1>;
					SENSOR-supply = <&reg_sensor>;
					gpio_pmic_pwrgood = <&gpio2 21 0>;
					gpio_pmic_vcom_ctrl = <&gpio3 17 0>;
					gpio_pmic_wakeup = <&gpio3 20 0>;
					gpio_pmic_v3p3 = <&gpio2 20 0>;
					gpio_pmic_intr = <&gpio2 25 0>;

					regulators {

						DISPLAY_reg: DISPLAY {
							regulator-name = "DISPLAY";
						};
						GVDD_reg: GVDD {
							/* 20v */
							regulator-name = "GVDD";
						};
						GVEE_reg: GVEE {
							/* -22v */
							regulator-name = "GVEE";
						};
						HVINN_reg: HVINN {
							/* -22v */
							regulator-name = "HVINN";
						};
						HVINP_reg: HVINP {
							/* 20v */
							regulator-name = "HVINP";
						};
						VCOM_reg: VCOM {
							regulator-name = "VCOM";
							/* 2’s-compliment, -4325000 */
							regulator-min-microvolt = <0xffbe0178>;
							/* 2’s-compliment, -500000 */
							regulator-max-microvolt = <0xfff85ee0>;
						};
						VNEG_reg: VNEG {
							/* -15v */
							regulator-name = "VNEG";
						};
						VPOS_reg: VPOS {
							/* 15v */
							regulator-name = "VPOS";
						};
						V3P3_reg: V3P3 {
							regulator-name = "V3P3";
						};
					};
				};
			};

			mipi@021dc000 { /* MIPI-CSI */
				status = "okay";
				ipu_id = <0>;
				csi_id = <1>;
				v_channel = <0>;
				lanes = <2>;
			};

			vdoa@021e4000 {
				status = "okay";
			};
		};

		ipu1: ipu@02400000 { /* IPU1 */
			bypass_reset = <0>;
		};

	};

	cpufreq-setpoint {
			/*
			 * 1:bypass internal regulator,supplied by external reg
			 * 0:enable internal regulator
			 */
			bypass-mode = <1>;
			VDDARM-supply = <&sw1a_reg>;
			VDDSOC-supply = <&sw1c_reg>;
	};

	gpio-keys {
		compatible = "gpio-keys";

		power {
			label = "Power Button";
			gpios = <&gpio3 29 0>;
			linux,code = <116>; /* KEY_POWER */
			gpio-key,wakeup;
		};

		volume-up {
			label = "Volume Up";
			gpios = <&gpio1 4 0>;
			linux,code = <115>; /* KEY_VOLUMEUP */
		};

		volume-down {
			label = "Volume Down";
			gpios = <&gpio1 5 0>;
			linux,code = <114>; /* KEY_VOLUMEDOWN */
		};
	};

	regulators {
		compatible = "simple-bus";

		reg_audio: 8962_supply {
			compatible = "regulator-fixed";
			regulator-name = "8962-SUPPLY";
			gpio = <&gpio4 10 0>;
			enable-active-high;
		};

		aux_5v: aux5v {
			compatible = "regulator-fixed";
			regulator-name = "AUX_5V";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			gpio = <&gpio6 10 1>;
			regulator-boot-on;
			enable-active-high;
		};

		reg_usb_otg_vbus: usb_otg_vbus {
			compatible = "regulator-fixed";
			regulator-name = "usb_otg_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			gpio = <&gpio3 22 0>;
			enable-active-high;
		};

		reg_usb_h1_vbus: usb_h1_vbus {
			compatible = "regulator-fixed";
			regulator-name = "usb_h1_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			gpio = <&gpio1 29 0>;
			enable-active-high;
		};

		reg_sensor: sensor_supply {
			compatible = "regulator-fixed";
			regulator-name = "sensor-SUPPLY";
			gpio = <&gpio2 31 0>;
			enable-active-high;
		};
	};

	sound-hdmi {
		compatible = "fsl,imx6q-sabresd-hdmi-audio",
			     "fsl,imx-audio-hdmi";
		model = "imx-audio-hdmi";
	};

	sound {
		compatible = "fsl,imx6q-sabresd-wm8962",
			     "fsl,imx-audio-wm8962";
		model = "wm8962-audio";
		ssi-controller = <&ssi2>;
		audio-codec = <&codec>;

		audio-routing =
			"Headphone Jack", "HPOUTL",
			"Headphone Jack", "HPOUTR",
			"Ext Spk", "SPKOUTL",
			"Ext Spk", "SPKOUTR",
			"MICBIAS", "AMIC",
			"IN3R", "MICBIAS",
			"DMIC", "MICBIAS",
			"DMICDAT", "DMIC";
		mux-int-port = <2>;
		mux-ext-port = <3>;
		hp-det-gpios = <&gpio7 8 1>;	/*active low*/
		mic-det-gpios = <&gpio1 9 1>;	/*active low*/
	};

	battery {
		compatible = "fsl,max8903-charger";
		pinctrl-names = "default";
		dok_input = <&gpio2 24 1>;
		uok_input = <&gpio1 27 1>;
		chg_input = <&gpio3 23 1>;
		flt_input = <&gpio5 2 1>;
		fsl,dcm_always_high;
		fsl,dc_valid;
		fsl,usb_valid;
		fsl,imx6q;
		status = "okay";
	};

	backlight {
		compatible = "pwm-backlight";
		pwms = <&pwm1 0 5000000>;
		brightness-levels = <0 4 8 16 32 64 128 255>;
		default-brightness-level = <7>;
	};

	mxcfb1: fb@0 {
		compatible = "fsl,mxc_sdc_fb";
		disp_dev = "ldb";
		interface_pix_fmt = "RGB666";
		mode_str ="LDB-XGA";
		default_bpp = <16>;
		int_clk = <0>;
		late_init = <0>;
		status = "disabled";
	};

	mxcfb2: fb@1 {
		compatible = "fsl,mxc_sdc_fb";
		disp_dev = "ldb";
		interface_pix_fmt = "RGB666";
		mode_str ="LDB-XGA";
		default_bpp = <16>;
		int_clk = <0>;
		late_init = <0>;
		status = "disabled";
	};

	mxcfb3: fb@2 {
		compatible = "fsl,mxc_sdc_fb";
		disp_dev = "lcd";
		interface_pix_fmt = "RGB565";
		mode_str ="CLAA-WVGA";
		default_bpp = <16>;
		int_clk = <0>;
		late_init = <0>;
		status = "disabled";
	};

	mxcfb4: fb@3 {
		compatible = "fsl,mxc_sdc_fb";
		disp_dev = "ldb";
		interface_pix_fmt = "RGB666";
		mode_str ="LDB-XGA";
		default_bpp = <16>;
		int_clk = <0>;
		late_init = <0>;
		status = "disabled";
	};

	lcd@0 {
		compatible = "fsl,lcd";
		ipu_id = <0>;
		disp_id = <0>;
		default_ifmt = "RGB565";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_ipu1_1>;
		status = "okay";
	};

	v4l2_cap_0 {
		compatible = "fsl,imx6q-v4l2-capture";
		ipu_id = <0>;
		csi_id = <0>;
		mclk_source = <0>;
		status = "okay";
	};

	v4l2_cap_1 {
		compatible = "fsl,imx6q-v4l2-capture";
		ipu_id = <0>;
		csi_id = <1>;
		mclk_source = <0>;
		status = "okay";
	};

	v4l2_out {
		compatible = "fsl,mxc_v4l2_output";
		status = "okay";
	};

	lvds_cabc_ctrl {
		lvds0-gpios = <&gpio6 15 0>;
		lvds1-gpios = <&gpio6 16 0>;
	};

	pxp_v4l2_out {
		compatible = "fsl,imx6dl-pxp-v4l2";
		status = "disabled";
	};
	pxp_v4l2_out {
		compatible = "fsl,imx6dl-pxp-v4l2";
		status = "disabled";
	};
};
