			label = "reset-request";
			gpios = GP_GPIOKEY_RESET_REQUEST;
			linux,code = <KEY_FN_F1>;
		};

		j24-oc {
			label = "j24-oc";
			gpios = GP_GPIOKEY_J24_OC;
			linux,code = <KEY_FN_F2>;
		};
