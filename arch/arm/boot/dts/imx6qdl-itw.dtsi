	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio_leds>;
		green {
			gpios = GP_GPIOLEDS_GREEN;
			retain-state-suspended;
			default-state = "off";
		};
		red {
			gpios = GP_GPIOLEDS_RED;
			retain-state-suspended;
			default-state = "off";
		};
		sata {
			gpios = GP_GPIOLEDS_SATA;
			retain-state-suspended;
			default-state = "off";
		};
		usb3 {
			gpios = GP_GPIOLEDS_USB3;
			retain-state-suspended;
			default-state = "off";
