	cs-gpios = GP_ECSPI1_NOR_CS;
	status = "okay";

	flash: m25p80@0 {
		compatible = "sst,sst25vf016b";
		spi-max-frequency = <20000000>;
		reg = <0>;
		#address-cells = <1>;
		#size-cells = <1>;
		mtd@00000000 {
			label = "U-Boot";
			reg = <0x0 0xC0000>;
		};

		mtd@000C0000 {
			label = "env";
			reg = <0xC0000 0x2000>;
		};
		mtd@000C2000 {
			label = "spare";
			reg = <0xC2000 0x3e000>;
		};
		mtd@00100000 {
			label = "mfgSection";
			reg = <0x100000 0x10000>;
		};
		mtd@00110000 {
			label = "scaleHWConfig";
			reg = <0x110000 0x10000>;
		};
		mtd@00120000 {
			label = "scaleSWConfig";
			reg = <0x120000 0x40000>;
		};
		mtd@00160000 {
			label = "scaleKeys";
			reg = <0x160000 0x80000>;
		};
		mtd@001E0000 {
			label = "scale_unused";
			reg = <0x1e0000 0x20000>;
		};
	};
};

