	dsi-lanes = <4>;
	mode-skip-eot;
	mode-video;
	mode-video-burst;
/*	mode-video-hse; */
#if 0
	mipi-cmds-init = /bits/ 8 <
		GEN_S1P(0x83,0x00)	/* Page 0 */
		GEN_S1P(0x84,0x00)

		GEN_S1P(0x8F,0xA5)
		DELAY(5)
		DCS_S0P(0x01)		/* Reset */
		DELAY(20)
		GEN_S1P(0x8F,0xA5)
		GEN_S1P(0x83,0xAA)	/* Page 1 */
		GEN_S1P(0x84,0x11)
		GEN_S1P(0xA9,0x4B)

		GEN_S1P(0x83,0x00)	/* Page 0 */
